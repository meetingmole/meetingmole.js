{
	"source": {
		"includePattern": "(\\.d)?\\.ts$"
	},
	"plugins": [
		"node_modules/ts2jsdoc/plugin.js"
	],
	"opts": {
		"template": "node_modules/ts2jsdoc/template"
	},
	"typescript": {
		"target": "es5",
		"module": "amd"
	},
	"templates": {
		"systemName": "meetingmole.js",
		"theme": "default",
		"navType": "vertical",
		"inverseNav": false,
		"tableOfContents": true,
		"linenums": false,
		"collapseSymbols": true,
		"footer": "string",
		"copyright": "string",
		"analytics": "",
		"outputSourceFiles": "false",
		"outputSourcePath": "false"
	}
}
